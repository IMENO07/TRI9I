<?xml version="1.0" encoding="UTF-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".reservationDelete"
    android:orientation="vertical">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearAddcar"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearColumnclose"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylepurple_A700">

            <FrameLayout android:gravity="start"
                android:id="@+id/frameBackground"
                android:layout_height="@dimen/_709pxv"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangleEleven"
                    android:layout_gravity="start"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle11"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle11"/>

                <LinearLayout
                    android:id="@+id/linearColumnrectangleeight"
                    android:layout_width="362dp"
                    android:layout_height="643dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_marginTop="@dimen/_40pxv"
                    android:layout_marginEnd="@dimen/_35pxh"
                    android:layout_marginBottom="@dimen/_40pxv"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/linearColumndate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_59pxv"
                        android:layout_marginEnd="@dimen/_6pxh"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/linearColumneye"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:gravity="start"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/linearRowtime"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:id="@+id/linearRoweye"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.68"
                                    android:orientation="horizontal">

                                    <ImageButton
                                        android:id="@+id/btnEye"
                                        android:layout_width="@dimen/_24pxh"
                                        android:layout_height="@dimen/_24pxh"
                                        android:background="@drawable/rectangle_bg_white_a700_radius_12"
                                        android:paddingHorizontal="@dimen/_3pxh"
                                        android:paddingVertical="@dimen/_3pxv"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/img_eye"
                                        tools:ignore="ContentDescription" />

                                    <TextView
                                        android:id="@+id/departure"
                                        style="@style/txtPoppinsmedium14_2"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_13pxh"
                                        android:layout_marginTop="@dimen/_2pxv"
                                        android:layout_marginBottom="@dimen/_1pxv"
                                        android:layout_weight="0.73"
                                        android:ellipsize="end"
                                        android:singleLine="true" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linearRowtimeOne"
                                android:layout_width="351dp"
                                android:layout_height="71dp"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_marginEnd="@dimen/_1pxh"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:id="@+id/linearRowlocation"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.67"
                                    android:orientation="horizontal">

                                    <FrameLayout
                                        android:id="@+id/frameStacklocation"
                                        android:layout_width="@dimen/_18pxh"
                                        android:layout_height="@dimen/_53pxv"
                                        android:layout_marginBottom="@dimen/_1pxv">

                                        <ImageView
                                            android:id="@+id/imageLocation"
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/_24pxv"
                                            android:layout_gravity="bottom|start"
                                            android:layout_marginTop="@dimen/_10pxv"
                                            android:scaleType="fitXY"
                                            android:src="@drawable/img_location_pink_500"
                                            tools:ignore="ContentDescription"
                                            tools:src="@drawable/img_location_pink_500" />

                                        <View
                                            android:id="@+id/lineLineTwo"
                                            android:layout_width="1dp"
                                            android:layout_height="@dimen/_31pxv"
                                            android:layout_gravity="top|end"
                                            android:layout_marginStart="@dimen/_10pxh"
                                            android:layout_marginEnd="@dimen/_7pxh"
                                            android:layout_marginBottom="@dimen/_10pxv"
                                            android:background="@color/gray_400" />
                                    </FrameLayout>

                                    <TextView
                                        android:id="@+id/destination"
                                        style="@style/txtPoppinsmedium14_2"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_15pxh"
                                        android:layout_marginTop="@dimen/_33pxv"
                                        android:layout_weight="0.77"
                                        android:ellipsize="end"
                                        android:singleLine="true" />
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/time"
                                    style="@style/txtPoppinsmedium12_1"
                                    android:layout_width="16dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_143pxh"
                                    android:layout_marginTop="@dimen/_34pxv"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_weight="0.33"
                                    android:ellipsize="end"
                                    android:singleLine="true"
                                    android:textSize="16dp" />
                            </LinearLayout>

                            <TextView
                                android:id="@+id/textView3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20px"
                                android:layout_marginBottom="20px"
                                android:text="conditions of the trip :" />

                            <TextView
                                android:id="@+id/condition"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" />

                            <LinearLayout
                                android:id="@+id/linearRowcar1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:id="@+id/linearRowcar2"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_weight="0.46"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:id="@+id/imageCar"
                                        android:layout_width="@dimen/_21pxh"
                                        android:layout_height="@dimen/_18pxh"
                                        android:layout_marginTop="30px"
                                        android:layout_marginBottom="@dimen/_1pxv"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/img_car"
                                        tools:ignore="ContentDescription"
                                        tools:src="@drawable/img_car" />

                                    <TextView
                                        android:id="@+id/carbrand"
                                        style="@style/txtPoppinsmedium14_2"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_14pxh"
                                        android:layout_marginTop="30px"
                                        android:layout_weight="0.63"
                                        android:ellipsize="end"
                                        android:singleLine="true" />
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/plaque"
                                    style="@style/txtSolidRounded"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_135pxh"
                                    android:layout_marginTop="30px"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_weight="0.54"
                                    android:ellipsize="end"
                                    android:paddingStart="@dimen/_6pxh"
                                    android:paddingTop="@dimen/_3pxv"
                                    android:paddingEnd="@dimen/_2pxh"
                                    android:paddingBottom="@dimen/_1pxv"
                                    android:singleLine="true" />
                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="20dp"
                        android:layout_marginTop="20px"
                        android:layout_marginBottom="20px"
                        android:text="numbre of places reserved :" />

                    <TextView
                        android:id="@+id/placestxt"
                        android:layout_width="match_parent"
                        android:layout_height="27dp"
                        android:textSize="14dp" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/deleteReservation"
                        style="@style/btnSolidRounded_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_90pxv"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_20pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_20pxv"
                        android:singleLine="true"
                        android:text="delete reservation"
                        android:textAllCaps="false" />
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>