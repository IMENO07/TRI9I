<?xml version="1.0" encoding="UTF-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BA9292"
    android:orientation="vertical"
    tools:context=".CarListEdit">

    <LinearLayout
        android:id="@+id/linearAddcar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearColumnclose"
            style="@style/groupStylepurple_A700"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/frameBackground"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_709pxv"
                android:layout_marginTop="@dimen/_20pxv"
                android:gravity="start">

                <ImageView
                    android:id="@+id/imageRectangleEleven"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle11"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_rectangle11" />

                <LinearLayout
                    android:id="@+id/linearColumndestination"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|center"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_54pxv"
                    android:layout_marginEnd="@dimen/_35pxh"
                    android:layout_marginBottom="@dimen/_34pxv"
                    android:orientation="vertical">


                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/backToMenu"
                        android:layout_width="match_parent"
                        android:layout_height="465dp" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/button"
                        style="@style/btnSolidRounded"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_33pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="back to profil"
                        android:textAllCaps="true"
                        />

                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>