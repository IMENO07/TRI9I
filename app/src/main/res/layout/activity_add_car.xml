<?xml version="1.0" encoding="UTF-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddCar"
    android:orientation="vertical">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearAddcar"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical">
        <LinearLayout android:id="@+id/linearColumnclose"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical"
            style="@style/groupStylepurple_A700">

            <FrameLayout android:gravity="start"
                android:id="@+id/frameBackground"
                android:layout_height="@dimen/_709pxv"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent">

                <LinearLayout
                    android:id="@+id/linearColumndestination"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="70dp"
                    android:layout_marginEnd="@dimen/_35pxh"
                    android:layout_marginBottom="@dimen/_54pxv"
                    android:orientation="vertical">

                    <EditText
                        android:id="@+id/conditionEditText"
                        style="@style/etSolidRoundedOutline"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:background="@drawable/rectangle_bg_gray_50_border_gray_200_radius_24"
                        android:ellipsize="end"
                        android:hint="Registration number"
                        android:inputType="number"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_13pxv"
                        android:singleLine="true"
                        android:textColorHint="@color/gray_700"
                        tools:ignore="Autofill,TextSizeCheck" />

                    <EditText
                        android:id="@+id/brandCar"
                        style="@style/etSolidRoundedOutline"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:background="@drawable/rectangle_bg_gray_50_border_gray_200_radius_24"
                        android:ellipsize="end"
                        android:hint="Brand"
                        android:inputType="text"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_16pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_10pxv"
                        android:singleLine="true"
                        android:textColorHint="@color/gray_700"
                        tools:ignore="Autofill,TextSizeCheck" />

                    <EditText
                        android:id="@+id/modalCar"
                        style="@style/etSolidRoundedOutline"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:background="@drawable/rectangle_bg_gray_50_border_gray_200_radius_24"
                        android:ellipsize="end"
                        android:hint="Modele"
                        android:inputType="text"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_13pxv"
                        android:singleLine="true"
                        android:textColorHint="@color/gray_700"
                        tools:ignore="Autofill,TextSizeCheck" />

                    <EditText
                        android:id="@+id/placesCar"
                        style="@style/etSolidRoundedOutline"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:background="@drawable/rectangle_bg_gray_50_border_gray_200_radius_24"
                        android:ellipsize="end"
                        android:hint="places"
                        android:inputType="number"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_13pxv"
                        android:singleLine="true"
                        android:textColorHint="@color/gray_700"
                        tools:ignore="Autofill,TextSizeCheck" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/pushCarButton"
                        style="@style/btnSolidRounded_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_180pxv"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_20pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_20pxv"
                        android:singleLine="true"
                        android:text="Add"
                        android:textAllCaps="false" />

                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>